package entity.order;

public class OrderPage {
	private Integer  pageSize=3;//默认每次查多少条数据
	private Integer pageNo=1;//当前第一页
	private Integer pogeCount;//总页数
	private Integer sum;//总条数
	public Integer getPageSize(){
		return pageSize;
	}
	
	public void setPageSize(Integer pageSize){
		this.pageSize = pageSize;
	}
	public Integer getPageNo(){
		return pageNo;
	}
	public void setPageNo(Integer pageNo){
		this.pageNo = pageNo;
	}
	public Integer getPogeCount(){
		return pogeCount;
	}
	public void setPogeCount(Integer pogeCount){
		this.pogeCount = pogeCount;
	}
	public Integer getSum(){
		return sum;
	}
	public void setSum(Integer sum) {
		this.sum = sum;
	}
}
