package entity.goodscomment;

public class GoodsCommentEntity {
	private int id;
	private String userCode;//用户编号
	private String commentDescride;//描述
	private int commentGrade;//评价等级状态
	private String commentImg;//晒单图片
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getUserCode() {
		return userCode;
	}
	public void setUserCode(String userCode) {
		this.userCode = userCode;
	}
	public String getCommentDescride() {
		return commentDescride;
	}
	public void setCommentDescride(String commentDescride) {
		this.commentDescride = commentDescride;
	}
	public int getCommentGrade() {
		return commentGrade;
	}
	public void setCommentGrade(int commentGrade) {
		this.commentGrade = commentGrade;
	}
	public String getCommentImg() {
		return commentImg;
	}
	public void setCommentImg(String commentImg) {
		this.commentImg = commentImg;
	}
	@Override
	public String toString() {
		return "Goods_comment [id=" + id + ", userCode=" + userCode + ", commentDescride=" + commentDescride
				+ ", commentGrade=" + commentGrade + ", commentImg=" + commentImg + "]";
	}
	
	
}
